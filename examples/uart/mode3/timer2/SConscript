Import('env')

env.Command('uart1_9bit_shared.c', '#src/uart1_9bit_shared.c', Copy('$TARGET', '$SOURCE'))

env.Default(
    env.Program(
        'uart1_mode3_timer2_12T_example',
        ['uart1_mode3_timer2_12T_example.c', 'uart1_9bit_shared.c']
    ),

    env.Program(
        'uart1_mode3_timer2_12T_freq_bias_example',
        ['uart1_mode3_timer2_12T_freq_bias_example.c']
    ),

    env.Program(
        'uart1_mode3_timer2_12T_interrupt_example',
        ['uart1_mode3_timer2_12T_interrupt_example.c']
    ),

)